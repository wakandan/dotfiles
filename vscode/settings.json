{
  "AllAutocomplete.minWordLength": 1,
  "flake8.args": [
    "--max-line-length=120"
  ],
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true,
    "editor.wordBasedSuggestionsMode": "allDocuments"
  },
  "autoDocstring.docstringFormat": "sphinx",
  "calva.prettyPrintingOptions": {
    "enabled": false,
    "maxLength": 50,
    "printEngine": "pprint",
    "width": 120
  },
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "other": "on",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestionsMode": "currentDocument",
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.hcl": "terraform",
    "*.kt": "kotlin",
    "*.toml": "toml"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "files.insertFinalNewline": true,
  "git.openRepositoryInParentFolders": "never",
  "go.toolsManagement.autoUpdate": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.enable": false,
  "prettier.requireConfig": true,
  "python.analysis.inlayHints.callArgumentNames": false,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": false,
  "python.languageServer": "Pylance",
  "python.linting.flake8Args": [
    "--ignore=E501,W503,E203"
  ],
  "redhat.telemetry.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.inheritEnv": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "none",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.maxItemsComputed": 50000,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "debug.javascript.autoAttachFilter": "disabled",
  "terminal.external.windowsExec": "C:\\Users\\khoa\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "workbench.colorTheme": "Visual Studio Light",
  "[terraform]": {
    "editor.tabSize": 2
  }
}